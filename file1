#include <iostream>
#include <algorithm>

void sortArray(int arr[], int size) {
    std::sort(arr, arr + size);
}

int main() {
    int numbers[] = {5, 2, 9, 1, 7};
    int size = sizeof(numbers) / sizeof(numbers[0]);
    sortArray(numbers, size);

    for (int i = 0; i < size; i++) {
        std::cout << numbers[i] << " ";
    }

    std::cout << std::endl;

    return 0;
}

